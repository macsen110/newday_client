        getLegalAddress: function (lineData, pointData, dataSource) {
            var tempLineData = [];
            var tempPointData = [];
            $.each(lineData, function (i, v) {
                var startAddress = v[0]["name"];
                var endAddress = v[1]["name"];
                if (!!dataSource[startAddress] && !!dataSource[endAddress]) {
                    tempLineData.push(v);
                    tempPointData.push(pointData[i]);
                }
            });
            return {
                lineData: tempLineData,
                pointData: tempPointData
            }
        },
        drawLineBar: function (option) {
