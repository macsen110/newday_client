.member-list>tbody>tr>td,.member-list>thead th{border:1px solid #ddd}
.member-list>tbody>tr:nth-child(even){background-color:#f6f6f6;border-left-color:#f6f6f6}
.member-list>tbody>tr:hover{background-color:#fff2b5;border-left-color:#f48400}
.member-list>tbody>tr>td{max-width:300px;overflow:hidden;-o-text-overflow:ellipsis;text-overflow:ellipsis;line-height:20px;padding:2px 8px}
.member-list>tbody>tr>td a{color:#2778cb}
.member-list>tbody>tr>td a.btn-default{color:#f48400}
.member-list input[type=checkbox]{display:inline-block;vertical-align:middle}
.bootstrap-table .fixed-table-body{overflow-x:scroll}
.bootstrap-table .fixed-table-pagination{margin:12px 0;display:inline-block}
.bootstrap-table .pagination-detail .dropdown-toggle{padding:0 5px;margin:0 5px;border-color:#ddd;border-radius:0}
.bootstrap-table .pagination{margin:0}
.bootstrap-table .pagination li{margin:0 5px}
.henrie .bootstrap-table .pagination li a { margin: auto !important; padding: 0 7px !important; }
.bootstrap-table .pagination li a{display:inline-block;padding:0 7px;height:24px;line-height:22px;color:#f48400;border:1px solid #e0e1e4}
.bootstrap-table .page-number.active a{border-color:#f48400;background-color:#f48400;color:#fff}
.bootstrap-table .ckd td{background-color:#bcffb6!important}
.bootstrap-table .btn-group { position: relative; }
.bootstrap-table .dropdown-menu{display:none; position:absolute; z-index: 100; top: auto; bottom: 100%; left:0; min-width: 120px; background-color: #fff; border: solid 1px #ddd; border-radius: 3px; }
.bootstrap-table .dropdown-menu li a { display: block; padding: 3px 20px; }
.bootstrap-table .dropdown-menu .active a { background-color: #2778cb; color: #fff; }
.bootstrap-table .open .dropdown-menu{display:block}
.pull-left{float:left!important}
.pull-right{float:right!important}
.bootstrap-table .pagination li{display:inline-block;vertical-align:middle}
.bootstrap-table .pagination li.disabled a { background-color: #f5f5f5; color: #ccc; cursor: not-allowed; }
.bootstrap-table .pagination li.disabled a:hover { text-decoration: none; }
.bootstrap-table .fixed-table-loading{position:absolute;z-index:100;top:0!important;right:0;bottom:0;left:0; text-align:center; background:rgba(255,255,255,.7) url(../img/loading.gif) no-repeat center center; font-size:0;}
.henrie .bootstrap-table .caret { border: none; }
.bootstrap-table .caret { display: inline-block; vertical-align: middle; width: 16px; height: 16px; background: url(../img/ico-sprite.png) no-repeat -65px -304px }