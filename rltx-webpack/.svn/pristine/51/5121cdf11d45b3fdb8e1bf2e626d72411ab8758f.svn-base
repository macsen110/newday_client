	}

	.map-footer {
		text-align: center;
	}

	.btn-grey {
	    background-color: #f6f6f6;
	    border-color: #e8e8e8;
	    color: #2778cb;
	}

	.btn-grey, .btn-light, .btn-org {
	    display: inline-block;
	    padding: 0 12px;
	    height: 22px;
	    line-height: 22px;
	    border: solid 1px #e8e8e8;
	    border-radius: 3px;
	    margin: 0 5px;
	}

	.truck-tag, .truck-role, .truck-status, .fee-type, .user-status, .od-status {
	    display: inline-block;
	    font-size: 12px;
	    height: 14px;
	    line-height: 14px;
	    padding: 0 1px;
	    background-color: #fff;
	    color: #f48400;
	    border: solid 1px #f48400;
	    border-radius: 3px;
	    margin-right: 8px;
	}

	.truck-role {
	    background-color: #ff9110;
	    color: #fff;
	}

	em {
		font-style: normal;
	}

	.copilot .truck-role {
	    background-color: #2c84de;
	    border-color: #2c84de;
	}
</style>
