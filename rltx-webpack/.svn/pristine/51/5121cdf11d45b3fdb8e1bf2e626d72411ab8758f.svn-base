	.txtbox, .dropdata {
	    width: 140px;
	    height: 22px;
	    padding: 0 5px;
	    line-height: 22px;
	    color: #464646;
	    border: solid 1px #dadada;
	    -webkit-transition: all .2s ease-in-out;
	    transition: all .2s ease-in-out;
	}

	input, textarea, select, button {
	    text-rendering: auto;
	    color: initial;
	    letter-spacing: normal;
	    word-spacing: normal;
	    text-transform: none;
	    text-indent: 0px;
	    text-shadow: none;
	    display: inline-block;
	    text-align: start;
	    margin: 0em;
	    font: 13.3333px Arial;
	}

	option {
	    font-weight: normal;
	    display: block;
	    white-space: pre;
	    min-height: 1.2em;
	    padding: 0px 2px 1px;
	}

	.s-btn {
	    background: #f48400;
	    width: 60px;
	    height: 26px;
	    line-height: 26px;
