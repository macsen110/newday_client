{
  "presets": [
    ["env", { "modules": false }],
    "es2015",
    "stage-2"
  ],
  "plugins": ["transform-runtime",["component", [
    {
      "libraryName": "element-ui",
      "styleLibraryName": "theme-default"
    }
  ]]],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env","es2015", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
